{
  "name": "news-dashboard",
  "type": "static",
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "installCommand": "npm install",
  "framework": "static",
  "nodeVersion": "18",
  "environmentVariables": {},
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/aurora",
      "dest": "/index-aurora.html"
    },
    {
      "src": "/daily-summary",
      "dest": "/daily-summary.html"
    },
    {
      "src": "/world-events",
      "dest": "/world-events.html"
    }
  ]
}